



<!DOCTYPE html>
<html lang="@UTW_Project.Helpers.CultureHelper.GetCurrentNeutralCulture()" dir="@(UTW_Project.Helpers.CultureHelper.IsRighToLeft() ? "rtl" : "ltr")">

@Styles.Render("~/Content/css" + (UTW_Project.Helpers.CultureHelper.IsRighToLeft() ? "-rtl" : ""))
@Scripts.Render("~/bundles/modernizr")
@*<html lang="en">*@
<head>
    <meta charset="utf-8" />
    <title>Capital Market</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicons -->
    <link href="~/Content/eBusiness/img/favicon.png" rel="icon">
    <link href="~/Content/eBusiness/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,400i,600,700|Raleway:300,400,400i,500,500i,700,800,900" rel="stylesheet">

    <!-- Bootstrap CSS File -->
    <link href="~/Content/eBusiness/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Libraries CSS Files -->
    <link href="~/Content/eBusiness/lib/nivo-slider/css/nivo-slider.css" rel="stylesheet">
    <link href="~/Content/eBusiness/lib/owlcarousel/owl.carousel.css" rel="stylesheet">
    <link href="~/Content/eBusiness/lib/owlcarousel/owl.transitions.css" rel="stylesheet">
    <link href="~/Content/eBusiness/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/eBusiness/lib/animate/animate.min.css" rel="stylesheet">
    <link href="~/Content/eBusiness/lib/venobox/venobox.css" rel="stylesheet">
    <link href="~/content/datatables/css/jquery.datatables.css" rel="stylesheet" />
    <script src="~/Content/eBusiness/lib/jquery/jquery.min.js"></script>
    @Scripts.Render("~/bundles/jquery")

    <!-- Nivo Slider Theme -->
    <link href="~/Content/eBusiness/css/nivo-slider-theme.css" rel="stylesheet">

    <!-- Main Stylesheet File -->
    <link href="~/Content/eBusiness/css/style.css" rel="stylesheet">

    <!-- Responsive Stylesheet File -->
    <link href="~/Content/eBusiness/css/responsive.css" rel="stylesheet">

    <!-- =======================================================
      Theme Name: eBusiness
      Theme URL: https://bootstrapmade.com/ebusiness-bootstrap-corporate-template/
      Author: BootstrapMade.com
      License: https://bootstrapmade.com/license/
    ======================================================= -->
    <link href="~/content/pickday/pikaday.css" rel="stylesheet" />
    
    <link href="~/content/pickday/theme.css" rel="stylesheet" />
    

</head>


<body data-spy="scroll" data-target="#navbar-example">



    <header>
        <!-- header-area start -->
        <div id="sticker" class="header-area">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 col-sm-12">

                        <!-- Navigation -->
                        <nav class="navbar navbar-default">
                            <!-- Brand and toggle get grouped for better mobile display -->
                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".bs-example-navbar-collapse-1" aria-expanded="false">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                                <!-- Brand -->
                                <a class="navbar-brand page-scroll sticky-logo" href="/Home/Index">
                                    <h1><span>capital</span>Market</h1>
                                    <!-- Uncomment below if you prefer to use an image logo -->
                                    <!-- <img src="img/logo.png" alt="" title=""> -->
                                </a>
                            </div>
                            <!-- Collect the nav links, forms, and other content for toggling -->
                            <div class="collapse navbar-collapse main-menu bs-example-navbar-collapse-1" id="navbar-example">
                                <ul class="nav navbar-nav navbar-right">

                                    @if (Session["ID"] != null)
                                    {
                                        if (Convert.ToString(Session["Role"]).Equals("Admin"))
                                        {
                                            <li>
                                                <a class="page-scroll" href="/Admin/Index">@UTW_Project.Resources.MyTexts.Users</a>
                                            </li>
                                        }

                                        if (Convert.ToString(Session["Role"]).Equals("User"))
                                        {

                                            <li>
                                                <a class="page-scroll" href="/Order/OrderIndex">@UTW_Project.Resources.MyTexts.Order</a>
                                            </li>
                                            <li>
                                                <a class="page-scroll" href="/Dashboard/Dashboard">@UTW_Project.Resources.MyTexts.Dashboard</a>
                                            </li>

                                        }
                                        <li>
                                            <a class="page-scroll" href="/Monitor/Orders">@UTW_Project.Resources.MyTexts.Monitor</a>
                                        </li>
                                        <li>
                                            <a class="page-scroll">Welcome @Session["UserName"]</a>
                                        </li>
                                        <li>
                                            <a class="page-scroll" href="/Home/Logout">@UTW_Project.Resources.MyTexts.LogOut</a>
                                        </li>
                                    }
                                    else
                                    {
                                        <li>
                                            <a class="page-scroll" href="/Home/LogIn">@UTW_Project.Resources.MyTexts.LogIn</a>
                                        </li>
                                        <li>
                                            <a class="page-scroll" href="/Home/Registration">@UTW_Project.Resources.MyTexts.SignUP</a>
                                        </li>

                                    }
                                    <li>

                                        @Html.ActionLink("English", "Change", "Language", new { languageAbbreviation = "en" }, null)



                                    </li>
                                    <li>

                                        @Html.ActionLink("العربية", "Change", "Language", new { languageAbbreviation = "ar" }, null)



                                    </li>

                                </ul>
                            </div>                            <!-- navbar-collapse -->
                        </nav>
                        <!-- END: Navigation -->
                    </div>
                </div>
            </div>
        </div>
        <!-- header-area end -->
    </header>
    <!-- header end -->

    <div class="container body-content">
        @RenderBody()
        <hr />
        <!-- Start Footer bottom Area -->
        <footer>
            <div class="footer-area">
                <div class="container">
                    <div class="row">
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <div class="footer-content">
                                <div class="footer-head">
                                    <div class="footer-logo">
                                        <h2><span>Capital</span>Market</h2>
                                    </div>

                                    <p> We seek to cover most transactions for capital market .</p>
                                    <div class="footer-icons">
                                        <ul>
                                            <li>
                                                <a href="#"><i class="fa fa-facebook"></i></a>
                                            </li>
                                            <li>
                                                <a href="#"><i class="fa fa-twitter"></i></a>
                                            </li>
                                            <li>
                                                <a href="#"><i class="fa fa-google"></i></a>
                                            </li>
                                            <li>
                                                <a href="#"><i class="fa fa-pinterest"></i></a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- end single footer -->
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <div class="footer-content">
                                <div class="footer-head">
                                    <h4>information</h4>
                                    <p>
                                        Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor.
                                    </p>
                                    <div class="footer-contacts">
                                        <p><span>Tel:</span> +123 456 789</p>
                                        <p><span>Email:</span> contact@example.com</p>
                                        <p><span>Working Hours:</span> 9am-5pm</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- end single footer -->
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <div class="footer-content">
                                <div class="footer-head">
                                    <h4>Instagram</h4>
                                    <div class="flicker-img">
                                        <a href="#"><img src="~/Content/eBusiness/img/portfolio/1.jpg" alt=""></a>
                                        <a href="#"><img src="~/Content/eBusiness/img/portfolio/2.jpg" alt=""></a>
                                        <a href="#"><img src="~/Content/eBusiness/img/portfolio/3.jpg" alt=""></a>
                                        <a href="#"><img src="~/Content/eBusiness/img/portfolio/4.jpg" alt=""></a>
                                        <a href="#"><img src="~/Content/eBusiness/img/portfolio/5.jpg" alt=""></a>
                                        <a href="#"><img src="~/Content/eBusiness/img/portfolio/6.jpg" alt=""></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="footer-area-bottom">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="copyright text-center">
                                <p>
                                    &copy; Copyright <strong>Capital Market</strong>. All Rights Reserved
                                </p>
                            </div>
                            <div class="credits">
                                <!--
                                  All the links in the footer should remain intact.
                                  You can delete the links only if you purchased the pro version.
                                  Licensing information: https://bootstrapmade.com/license/
                                  Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/buy/?theme=eBusiness
                                -->
                                @*Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a>*@
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    <!-- JavaScript Libraries -->

    <script src="~/Content/eBusiness/lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/eBusiness/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/Content/eBusiness/lib/venobox/venobox.min.js"></script>
    <script src="~/Content/eBusiness/lib/knob/jquery.knob.js"></script>
    <script src="~/Content/eBusiness/lib/wow/wow.min.js"></script>
    <script src="~/Content/eBusiness/lib/parallax/parallax.js"></script>
    <script src="~/Content/eBusiness/lib/easing/easing.min.js"></script>
    <script src="~/Content/eBusiness/lib/nivo-slider/js/jquery.nivo.slider.js" type="text/javascript"></script>
    <script src="~/Content/eBusiness/lib/appear/jquery.appear.js"></script>
    <script src="~/Content/eBusiness/lib/isotope/isotope.pkgd.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD8HeI8o-c1NppZA-92oYlXakhDPYR7XMY"></script>
    <script src="~/content/ebusiness/contactform/contactform.js"></script>
    <script src="~/scripts/jquery-1.10.2.js"></script>
    <script src="~/content/ebusiness/js/main.js"></script>
    <script src="~/scripts/jquery-1.10.2.min.js"></script>
    <script src="~/scripts/exporting.js"></script>
    <script src="~/scripts/highcharts.js"></script>
    <script src="~/scripts/datatables/jquery.datatables.js"></script>
    <script src="~/scripts/pikaday.js"></script>
    <script src="~/content/pickday/jquery-ui.js"></script>
    <!-- Contact Form JavaScript File -->
    <script src="~/Content/eBusiness/contactform/contactform.js"></script>
    <script src="~/Content/eBusiness/lib/nivo-slider/js/jquery.nivo.slider.js"></script>

    <script src="~/Content/eBusiness/js/main.js"></script>


    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/bootstrap" + (UTW_Project.Helpers.CultureHelper.IsRighToLeft() ? "-rtl" : ""))
    @RenderSection("scripts", required: false)
</body>

</html>
